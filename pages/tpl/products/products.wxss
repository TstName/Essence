.template-wrap{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.template-main{
  min-height: 350rpx;
  width: 49%;
  margin-top: 10rpx;
  display: flex;
  flex-direction: column;
  background: #eee;
}
.template-img{
  width: 100%;
  height: 75%;
}
.template-name{
  text-align: center;
  font-size: 18px;
}
.template-price{
  text-align: center;
  font-size: 12px;
  margin-top: 10rpx;
  color: red;
}